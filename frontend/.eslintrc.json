{
  "parser": "vue-eslint-parser",
  "parserOptions": { 
      "parser": "@typescript-eslint/parser" 
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "globals": {
    "document": true
  },
  "ignorePatterns":[

  ],
  "rules": {
    "@typescript-eslint/no-explicit-any": "warn",
    "@typescript-eslint/no-unused-vars": [
      "warn"
    ]
  }
}